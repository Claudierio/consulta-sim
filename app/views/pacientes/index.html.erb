<%= stylesheet_link_tag 'custom' %>

<p style="color: green"><%= notice %></p>

<h1>Pacientes</h1>

<div id="pacientes">
  <% @pacientes.each do |paciente| %>
    <%= render paciente %>
    <p>
      <%= link_to "Show this paciente", paciente %>
    </p>
  <% end %>
</div>

<button onclick="window.location.href='<%= new_paciente_path %>'">Novo paciente</button>
